{
  "name": "jordanhavard/laravel-sms-clicksend",
  "description": "ClickSend Notifications channel for Laravel 9",
  "keywords": [
    "laravel",
    "notifications",
    "clicksend",
    "sms"
  ],
  "homepage": "https://github.com/jordanhavard/laravel-sms-clicksend",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordan Havard",
      "role": "Developer",
      "email": "sirj.havard@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "clicksend/clicksend-php": "4.0.*",
    "guzzlehttp/guzzle": "^7.5"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^6.0",
    "nunomaduro/larastan": "^2.0.1",
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5",
    "spatie/laravel-ray": "^1.26"
  },
  "autoload": {
    "psr-4": {
      "JordanHavard\\ClickSend\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JordanHavard\\ClickSend\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "extra": {
      "sort-packages": true
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "JordanHavard\\ClickSend\\ClickSendServiceProvider"
      ]
    }
  }
}
