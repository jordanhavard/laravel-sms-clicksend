{
  "name": "jordanhavard/laravel-sms-clicksend",
  "description": "ClickSend Notifications channel for Laravel 5.1+",
  "keywords": [
    "laravel",
    "notifications",
    "clicksend",
    "sms"
  ],
  "homepage": "https://github.com/jordanhavard/laravel-sms-clicksend",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Kundrat",
      "role": "Developer",
      "email": "vlado@12345.com.au",
      "homepage": "https://github.com/vladski"
    },
    {
      "name": "Benjamin Ayles",
      "role": "Developer",
      "email": "ben.ayles@superloop.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.1",
    "clicksend/clicksend-php": "4.0.*",
    "illuminate/queue": "^8.0|^9.0",
    "illuminate/notifications": "^8.0|^9.0",
    "illuminate/support": "^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "NotificationChannels\\ClickSend\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotificationChannels\\ClickSend\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "extra": {
      "sort-packages": true
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "NotificationChannels\\ClickSend\\ClickSendServiceProvider"
      ]
    }
  }
}
